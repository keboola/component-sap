      "required": [
        "server_url",
        "username",
        "#password"
      ],
      "propertyOrder": 1,
      "properties": {
        "server_url": {
          "type": "string",
          "title": "Server URL",
          "description": "For example: https://sapserver.provider.cz:45300/sap/bc/http/sap/YACTUM_DP",
          "propertyOrder": 1
        },
        "username": {
          "type": "string",
          "title": "Username",
          "propertyOrder": 2
        },
        "#password": {
          "type": "string",
          "format": "password",
          "title": "Password",
          "propertyOrder": 3
        }
      }
    }
  }
}